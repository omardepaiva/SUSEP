// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
  
}

model Pessoa {
  id               Int     @id @default(autoincrement())
  nome             String
  cpf              String
  dataNascimento   DateTime
  email            String
  unidadeId        Int
  tipoFuncaoId     Int
  cargaHoraria     Int
  situacaoPessoa   Int
}
